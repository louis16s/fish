# Copy to openclaw/.env before deployment.

# PostgreSQL
POSTGRES_PASSWORD=CHANGE_ME_STRONG_DB_PASSWORD

# Web panel session signing (long random string)
SESSION_SECRET=CHANGE_ME_LONG_RANDOM

# First admin account (created only when no admin exists)
ADMIN_USERNAME=admin
ADMIN_PASSWORD=CHANGE_ME_STRONG_ADMIN_PASSWORD

# MQTT credentials used by the server (NOT the device). Create this user in EMQX.
MQTT_SERVER_USERNAME=fish_srv
MQTT_SERVER_PASSWORD=CHANGE_ME_STRONG_MQTT_PASSWORD

# Subscribe pattern for telemetry. If you only have one device, you can use:
# fish1/device/telemetry
# If you have multiple devices, use:
# +/device/telemetry
MQTT_TELEMETRY_SUB=+/device/telemetry

# Default device ID (topic first segment). Only used if the browser doesn't pass device_id.
DEFAULT_DEVICE_ID=fish1

# Data retention (days)
DATA_RETENTION_DAYS=30

